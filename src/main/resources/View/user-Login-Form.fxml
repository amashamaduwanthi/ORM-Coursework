<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loginPanel" prefHeight="649.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.Controller.UserLoginFormController">
   <children>
      <Label layoutX="128.0" layoutY="64.0" prefHeight="41.0" prefWidth="242.0" text="Login Form">
         <font>
            <Font name="System Bold" size="34.0" />
         </font>
      </Label>
      <TextField fx:id="txtUser" layoutX="67.0" layoutY="184.0" prefHeight="56.0" prefWidth="309.0" promptText="UserName">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtEmail" layoutX="67.0" layoutY="269.0" prefHeight="56.0" prefWidth="309.0" promptText="Email">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtPassword" layoutX="68.0" layoutY="348.0" prefHeight="56.0" prefWidth="309.0" promptText="Pssword">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <Button layoutX="102.0" layoutY="465.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="41.0" prefWidth="94.0" text="Login" />
      <Label layoutX="67.0" layoutY="536.0" prefHeight="16.0" prefWidth="152.0" text="Havent't an account    " textFill="#4c2da8" />
      <Button layoutX="223.0" layoutY="532.0" mnemonicParsing="false" onAction="#btnSinInOnAction" text="signin" />
   </children>
</AnchorPane>
